import java.io.BufferedReader;
import java.io.InputStreamReader;

public class Main {
    public static void main(String[] args) throws Exception {

        test x = new test();
        test2 w = new test2();
        x.start();
        w.start();
    }
}

class test extends Thread{
    public void run(){
        pint a = new pint("-");
        a.px();
    }
}

class test2 extends Thread{
    public void run(){
        pint a = new pint("*");
        a.px();
    }
}

class pint{
    String x;
    final int end = 100;
    pint(String x){
        this.x = x;
    }
     synchronized void px(){
        for(int i = 1; i<end; i++){
            System.out.println();
                for(int j=1; j < end; j++){
                    System.out.print(x);
                }
        }
    }
    
}
